generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("TENANT_DB_URL")
}

model attribute_groups {
  id                                       Int             @id(map: "attribute_group_id_pkey") @default(autoincrement()) @db.SmallInt
  name                                     String          @unique(map: "ukey_attribute_groups_name")
  key                                      String?         @unique(map: "ukey_attribute_groups_key") @db.VarChar(30)
  status                                   Int             @default(1) @db.SmallInt
  created_at                               DateTime        @default(now()) @db.Timestamptz(6)
  created_by                               String?         @db.Uuid
  updated_at                               DateTime?       @db.Timestamptz(6)
  updated_by                               String?         @db.Uuid
  deleted_at                               DateTime?       @db.Timestamptz(6)
  deleted_by                               String?         @db.Uuid
  users_attribute_groups_created_byTousers users?          @relation("attribute_groups_created_byTousers", fields: [created_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_attribute_groups_created_by_users")
  users_attribute_groups_deleted_byTousers users?          @relation("attribute_groups_deleted_byTousers", fields: [deleted_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_attribute_groups_deleted_by_users")
  users_attribute_groups_updated_byTousers users?          @relation("attribute_groups_updated_byTousers", fields: [updated_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_attribute_groups_updated_by_users")
  up_attributes                            up_attributes[]
}

model browsers {
  id                            String                          @id(map: "browser_pkey") @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name                          String                          @unique(map: "ukey_browsers_name") @db.VarChar(255)
  user_session_activities       user_session_activities[]
  user_session_activities_lives user_session_activities_lives[]
}

model builder_events {
  id                                     String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name                                   String    @db.VarChar(100)
  listener_id                            String    @db.Uuid
  expected_data                          Json?     @db.Json
  configuration                          Json?     @db.Json
  status                                 Int       @default(1) @db.SmallInt
  created_at                             DateTime  @default(now()) @db.Timestamptz(6)
  created_by                             String    @db.Uuid
  updated_at                             DateTime? @db.Timestamptz(6)
  updated_by                             String?   @db.Uuid
  deleted_at                             DateTime? @db.Timestamptz(6)
  deleted_by                             String?   @db.Uuid
  selector                               String?
  users_builder_events_created_byTousers users     @relation("builder_events_created_byTousers", fields: [created_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_builder_events_created_by_users")
  users_builder_events_deleted_byTousers users?    @relation("builder_events_deleted_byTousers", fields: [deleted_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_builder_events_deleted_by_users")
  users_builder_events_updated_byTousers users?    @relation("builder_events_updated_byTousers", fields: [updated_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_builder_events_updated_by_users")
}

model checklist_congratulations_content_lives {
  checklist_id                                                    String          @unique(map: "ukey_checklist_congratulations_content_lives_checklist") @db.Uuid
  is_congratulations_content_visible                              Boolean         @default(false)
  emoji                                                           String?
  image_url                                                       String?
  title                                                           String?         @db.VarChar(50)
  content                                                         String?
  created_at                                                      DateTime        @default(now()) @db.Timestamptz(6)
  created_by                                                      String          @db.Uuid
  updated_at                                                      DateTime?       @db.Timestamptz(6)
  updated_by                                                      String?         @db.Uuid
  is_animation_enabled                                            Boolean?        @default(false)
  checklist_lives                                                 checklist_lives @relation(fields: [checklist_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_congratulations_content_lives_checklist_id_checkli")
  users_checklist_congratulations_content_lives_created_byTousers users           @relation("checklist_congratulations_content_lives_created_byTousers", fields: [created_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_congratulations_content_lives_created_by_users")
  users_checklist_congratulations_content_lives_updated_byTousers users?          @relation("checklist_congratulations_content_lives_updated_byTousers", fields: [updated_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_congratulations_content_lives_updated_by_users")
}

model checklist_congratulations_contents {
  checklist_id                                               String     @unique(map: "ukey_checklist_congratulations_contents_checklist") @db.Uuid
  is_congratulations_content_visible                         Boolean    @default(false)
  emoji                                                      String?
  image_url                                                  String?
  title                                                      String?    @db.VarChar(50)
  content                                                    String?
  created_at                                                 DateTime   @default(now()) @db.Timestamptz(6)
  created_by                                                 String     @db.Uuid
  updated_at                                                 DateTime?  @db.Timestamptz(6)
  updated_by                                                 String?    @db.Uuid
  is_animation_enabled                                       Boolean?   @default(false)
  checklists                                                 checklists @relation(fields: [checklist_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_congratulations_contents_checklist_id_checklists")
  users_checklist_congratulations_contents_created_byTousers users      @relation("checklist_congratulations_contents_created_byTousers", fields: [created_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_congratulations_contents_created_by_users")
  users_checklist_congratulations_contents_updated_byTousers users?     @relation("checklist_congratulations_contents_updated_byTousers", fields: [updated_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_congratulations_contents_updated_by_users")
}

model checklist_item_lives {
  id                                           String              @id(map: "checklist_item_live_pkey") @db.Uuid
  checklist_id                                 String              @db.Uuid
  title                                        String?             @db.VarChar(255)
  is_go_to_page_enabled                        Boolean             @default(true)
  is_launch_flow_enabled                       Boolean             @default(false)
  go_to_page_url                               String?
  product_tour_id                              String?             @db.Uuid
  is_mark_completed_on_flow_start              Boolean             @default(false)
  is_mark_completed_on_condition               Boolean             @default(false)
  conditional_operator                         String?             @db.VarChar(5)
  checklist_item_conditions                    Json?               @db.Json
  sort_order                                   Int                 @db.SmallInt
  created_at                                   DateTime            @default(now()) @db.Timestamptz(6)
  created_by                                   String              @db.Uuid
  updated_at                                   DateTime?           @db.Timestamptz(6)
  updated_by                                   String?             @db.Uuid
  checklist_lives                              checklist_lives     @relation(fields: [checklist_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_item_lives_checklist_id_checklist_lives")
  users_checklist_item_lives_created_byTousers users               @relation("checklist_item_lives_created_byTousers", fields: [created_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_item_lives_created_by_users")
  product_tour_lives                           product_tour_lives? @relation(fields: [product_tour_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_item_lives_product_tour_id_product_tour_lives")
  users_checklist_item_lives_updated_byTousers users?              @relation("checklist_item_lives_updated_byTousers", fields: [updated_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_item_lives_updated_by_users")

  @@index([checklist_id], map: "idx_checklist_item_lives_checklist_id")
}

model checklist_items {
  id                                      String         @id(map: "checklist_item_pkey") @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  checklist_id                            String         @db.Uuid
  title                                   String?        @db.VarChar(255)
  is_go_to_page_enabled                   Boolean        @default(true)
  is_launch_flow_enabled                  Boolean        @default(false)
  go_to_page_url                          String?
  product_tour_id                         String?        @db.Uuid
  is_mark_completed_on_flow_start         Boolean        @default(false)
  is_mark_completed_on_condition          Boolean        @default(false)
  conditional_operator                    String?        @db.VarChar(5)
  checklist_item_conditions               Json?          @db.Json
  sort_order                              Int            @db.SmallInt
  created_at                              DateTime       @default(now()) @db.Timestamptz(6)
  created_by                              String         @db.Uuid
  updated_at                              DateTime?      @db.Timestamptz(6)
  updated_by                              String?        @db.Uuid
  checklists                              checklists     @relation(fields: [checklist_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_items_checklist_id_checklists")
  users_checklist_items_created_byTousers users          @relation("checklist_items_created_byTousers", fields: [created_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_items_created_by_users")
  product_tours                           product_tours? @relation(fields: [product_tour_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_items_product_tour_id_product_tours")
  users_checklist_items_updated_byTousers users?         @relation("checklist_items_updated_byTousers", fields: [updated_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_items_updated_by_users")

  @@index([checklist_id], map: "idx_checklist_items_checklist_id")
}

model checklist_lives {
  id                                      String                                   @id(map: "checklist_live_pkey") @db.Uuid
  name                                    String                                   @db.VarChar(100)
  description                             String?
  content_title                           String?                                  @db.VarChar(50)
  content_description                     String?                                  @db.VarChar(60)
  is_beacon_enabled                       Boolean                                  @default(true)
  beacon_text                             String?                                  @db.VarChar(30)
  has_dismiss_link                        Boolean                                  @default(true)
  prompt_text                             String?                                  @db.VarChar(50)
  cancel_button_text                      String?                                  @db.VarChar(50)
  confirm_button_text                     String?                                  @db.VarChar(50)
  is_segment_all_users                    Boolean                                  @default(true)
  segment_id                              String?                                  @db.Uuid
  is_environment_all                      Boolean                                  @default(true)
  selected_environments                   Json?                                    @db.Json
  status                                  Int                                      @default(1) @db.SmallInt
  created_at                              DateTime                                 @default(now()) @db.Timestamptz(6)
  created_by                              String                                   @db.Uuid
  updated_at                              DateTime?                                @db.Timestamptz(6)
  updated_by                              String?                                  @db.Uuid
  deleted_at                              DateTime?                                @db.Timestamptz(6)
  deleted_by                              String?                                  @db.Uuid
  has_goal                                Boolean                                  @default(false)
  goal_id                                 String?                                  @db.Uuid
  design                                  Json?                                    @db.Json
  is_anonymous_user                       Boolean?
  is_identified_user                      Boolean?
  checklist_congratulations_content_lives checklist_congratulations_content_lives?
  checklist_item_lives                    checklist_item_lives[]
  users_checklist_lives_created_byTousers users                                    @relation("checklist_lives_created_byTousers", fields: [created_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_lives_created_by_users")
  users_checklist_lives_deleted_byTousers users?                                   @relation("checklist_lives_deleted_byTousers", fields: [deleted_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_lives_deleted_by_users")
  goal                                    goal?                                    @relation(fields: [goal_id], references: [goal_id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_lives_goal_id_goal")
  segments                                segments?                                @relation(fields: [segment_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_lives_segment_id_segments")
  users_checklist_lives_updated_byTousers users?                                   @relation("checklist_lives_updated_byTousers", fields: [updated_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_lives_updated_by_users")
  checklist_trigger_lives                 checklist_trigger_lives?

  @@index([status], map: "idx_checklist_lives_status")
}

model checklist_trigger_lives {
  checklist_id                                    String            @id(map: "checklist_trigger_live_checklist_id_ukey") @db.Uuid
  is_all_pages                                    Boolean           @default(true)
  conditional_operator                            String?           @db.VarChar(5)
  checklist_trigger_conditions                    Json?             @db.Json
  display_trigger_id                              Int?              @db.SmallInt
  display_trigger_value                           String?           @db.VarChar(255)
  created_at                                      DateTime          @default(now()) @db.Timestamptz(6)
  created_by                                      String            @db.Uuid
  updated_at                                      DateTime?         @db.Timestamptz(6)
  updated_by                                      String?           @db.Uuid
  event_name                                      String?           @db.VarChar(255)
  is_triggered_by_event                           Boolean?          @default(false)
  checklist_lives                                 checklist_lives   @relation(fields: [checklist_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_trigger_lives_checklist_id_checklist_lives")
  users_checklist_trigger_lives_created_byTousers users             @relation("checklist_trigger_lives_created_byTousers", fields: [created_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_trigger_lives_created_by_users")
  display_triggers                                display_triggers? @relation(fields: [display_trigger_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_trigger_lives_display_trigger_id_display_triggers")
  users_checklist_trigger_lives_updated_byTousers users?            @relation("checklist_trigger_lives_updated_byTousers", fields: [updated_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_trigger_lives_updated_by_users")
}

