  id                                 String                              @id(map: "checklist_pkey") @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name                               String                              @db.VarChar(100)
  description                        String?
  content_title                      String?                             @db.VarChar(50)
  content_description                String?                             @db.VarChar(60)
  beacon_is_beacon_enabledstatus     Boolean                             @default(true)
  beacon_text                        String?                             @db.VarChar(30)
