  display_triggers                           display_triggers? @relation(fields: [display_trigger_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_triggers_display_trigger_id_display_triggers")
  users_checklist_triggers_updated_byTousers users?            @relation("checklist_triggers_updated_byTousers", fields: [updated_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_checklist_triggers_updated_by_users")
}

