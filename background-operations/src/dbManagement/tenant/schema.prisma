  deleted_at                               DateTime?       @db.Timestamptz(6)
  deleted_by                               String?         @db.Uuid
  users_condition_groups_created_byTousers users?          @relation("condition_groups_created_byTousers", fields: [created_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_condition_groups_created_by_users")
  users_condition_groups_deleted_byTousers users?          @relation("condition_groups_deleted_byTousers", fields: [deleted_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_condition_groups_deleted_by_users")
